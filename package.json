        "debug": false
      }
    },
    "travis-cov": {
      "threshold": 90
    }
  },
  "dependencies": {
    "forward": ">=0.0.4",
    "humanize-number": ">=0.0.2",
    "gravatar": "1.0.6",
    "urllib": ">=0.5.5",
    "connect-markdown": "0.0.3",
    "qn": ">=0.2.0",
    "microtime": "0.5.1",
    "debug": ">=0.7.4",
    "utility": ">=0.1.9",
    "ready": "0.1.1",
    "connect": "2.12.0",
    "connect-rt": "0.0.2",
    "connect-redis": "1.4.6",
    "connect-render": "0.3.2",
    "urlrouter": ">=0.5.4",
    "graceful": ">=0.0.5",
    "moment": "2.4.0",
    "logfilestream": ">=0.1.0",
    "ms": "0.6.2",
    "mkdirp": "0.3.5",
    "mysql": "2.0.0-rc2",
    "response-patch": "0.1.1",
    "response-cookie": "0.0.2",
